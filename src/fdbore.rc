MAINMENU MENU 
BEGIN
	POPUP "&File"
	BEGIN
		MENUITEM "&Open...\tCtrl+O", IDM_LOADFILE
		MENUITEM "Pro&ject open...\tCtrl+J", IDM_LOADPROJECT
		MENUITEM "&Save...\tCtrl+S", IDM_SAVEFILE, GRAYED
		MENUITEM "Save &as...", IDM_SAVEFILE_AS, GRAYED
        MENUITEM "&Print...\tCtrl+P", IDM_PRINT
		MENUITEM SEPARATOR
		MENUITEM "&Exit\tALT+F4", IDM_EXIT
	END

	POPUP "&Edit"
	BEGIN
		POPUP "&New System"
		BEGIN
			MENUITEM "&Lin. FIR...", IDM_NEWLINFIR
			MENUITEM "&Standard IIR...", IDM_NEWSTDIIR
			MENUITEM "&FIR/IIR Systems...", IDM_NEWFIRIIR
			MENUITEM "&Predefined Systems...", IDM_NEWPREDEF
		END

		MENUITEM "P&arameters...\tCtrl+A", IDM_DEFCHANGE, GRAYED
		MENUITEM "&Coefficients...", IDM_EDITCOEFF, GRAYED
		MENUITEM "&Project Information...", IDM_EDITPROJECT, GRAYED
		MENUITEM SEPARATOR
		MENUITEM "C&omments...", IDM_EDITCOMMENT, GRAYED
	END

	POPUP "&View"
	BEGIN
		MENUITEM "&Coefficients\tCtrl+C", IDM_SHOWCOEFF
		MENUITEM "&Roots\tCtrl+R", IDM_SHOWROOTS
	END

	POPUP "&Options"
	BEGIN
		MENUITEM "&Desktop...", IDM_OPT_DESK
		MENUITEM "&Math...", IDM_OPT_TECH
		MENUITEM SEPARATOR
		MENUITEM "&X-Axis...\tCtrl+X", IDM_OPT_AXIS_X, GRAYED
		MENUITEM "&Y-Axis...\tCtrl+Y", IDM_OPT_AXIS_Y, GRAYED
		MENUITEM SEPARATOR
		MENUITEM "Math. &Function...", IDM_OPT_USERFN, GRAYED
	END

	POPUP "&Window"
	BEGIN
		POPUP "&New"
		BEGIN
			MENUITEM "&Magnitude Response", IDM_NEW_DIAG_FRESP
			MENUITEM "&Attenuation", IDM_NEW_DIAG_ATTENUAT
            MENUITEM "&Discrimination", IDM_NEW_DIAG_APPROXRESP
			MENUITEM "&Phase Response", IDM_NEW_DIAG_PHASE
			MENUITEM "Phase De&lay", IDM_NEW_DIAG_PHASEDELAY
			MENUITEM "&Group Delay", IDM_NEW_DIAG_GROUPDELAY
			MENUITEM "&Impulse Response", IDM_NEW_DIAG_IMPULSRESP
			MENUITEM "&Step Response", IDM_NEW_DIAG_STEPRESP
			MENUITEM SEPARATOR
			MENUITEM "Math. &Function", IDM_NEW_DIAG_USERDEF
		END

		MENUITEM "&Repaint", IDM_DIAG_REPAINT, GRAYED
		MENUITEM SEPARATOR
		MENUITEM "&Close\tCtrl+F4", IDM_DIAG_CLOSE, GRAYED
		MENUITEM SEPARATOR
		MENUITEM "Tile &Vertical", IDM_DIAG_TILE_VERT, GRAYED
		MENUITEM "Tile &Horizontal", IDM_DIAG_TILE_HORIZ, GRAYED
		MENUITEM "&Cascade", IDM_DIAG_CASCADE, GRAYED
		MENUITEM "&Arrange Icons", IDM_ICON_ARRANGE, GRAYED
	END

	POPUP "&Help", HELP
	BEGIN
		MENUITEM "Inde&x\tUmschalt+F1", IDM_HELP_INDEX
		MENUITEM "&Usage", IDM_HELP_HELP
		MENUITEM SEPARATOR
		MENUITEM "&About...", IDM_HELP_ABOUT
		MENUITEM SEPARATOR
		MENUITEM "&Register...", IDM_HELP_REGISTER
	END

END


