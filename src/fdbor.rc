MAINMENU MENU 
{
 POPUP "&Datei"
 {
  MENUITEM "Ö&ffnen...\tStrg+O", IDM_LOADFILE
  MENUITEM "&Projekt öffnen...\tStrg+P", IDM_LOADPROJECT
  MENUITEM "&Speichern...\tStrg+S", IDM_SAVEFILE, GRAYED
  MENUITEM "Speichern &unter...", IDM_SAVEFILE_AS, GRAYED
  MENUITEM "&Drucken...\tStrg+D", IDM_PRINT
  MENUITEM SEPARATOR
  MENUITEM "&Beenden\tALT+F4", IDM_EXIT
 }

 POPUP "&Bearbeiten"
 {
  POPUP "&Neues System"
  {
   MENUITEM "&Lin. FIR...", IDM_NEWLINFIR
   MENUITEM "&Standard IIR...", IDM_NEWSTDIIR
   MENUITEM "&FIR/IIR Systeme...", IDM_NEWFIRIIR
   MENUITEM "&Vordefinierte...", IDM_NEWPREDEF
  }

  MENUITEM "&Ausgangsdaten...\tStrg+A", IDM_DEFCHANGE, GRAYED
  MENUITEM "&Koeffizienten...", IDM_EDITCOEFF, GRAYED
  MENUITEM "&Projektinformationen...", IDM_EDITPROJECT, GRAYED
  MENUITEM SEPARATOR
  MENUITEM "K&ommentare...", IDM_EDITCOMMENT, GRAYED
 }

 POPUP "&Anzeige"
 {
  MENUITEM "&Koeffizienten\tStrg+K", IDM_SHOWCOEFF
  MENUITEM "&Nullstellen/Pole\tStrg+N", IDM_SHOWROOTS
 }

 POPUP "&Optionen"
 {
  MENUITEM "&Desktop...", IDM_OPT_DESK
  MENUITEM "&Math...", IDM_OPT_TECH
