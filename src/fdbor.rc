   MENUITEM "Phasen&laufzeit", IDM_NEW_DIAG_PHASEDELAY
   MENUITEM "&Gruppenlaufzeit", IDM_NEW_DIAG_GROUPDELAY
   MENUITEM "&Impulsantwort", IDM_NEW_DIAG_IMPULSRESP
   MENUITEM "&Sprungantwort", IDM_NEW_DIAG_STEPRESP
   MENUITEM SEPARATOR
   MENUITEM "&Math. Funktion", IDM_NEW_DIAG_USERDEF
  }

  MENUITEM "Neu &Zeichnen", IDM_DIAG_REPAINT, GRAYED
  MENUITEM SEPARATOR
  MENUITEM "&Schließen\tStrg+F4", IDM_DIAG_CLOSE, GRAYED
